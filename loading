local function typewriterEffect(textLabel, text, speed)
	task.spawn(function()
		textLabel.Text = ""

		for i = 1, #text do
			textLabel.Text = string.sub(text, 1, i)
			task.wait(speed)
		end
	end)
end

local function animateParticles(particleContainer)
	local particles = particleContainer:GetChildren()
	local tweens = {}
	
	for _, particle in ipairs(particles) do
		local destinationX = math.random(-100, 100) / 100 + particle.Position.X.Scale
		local destinationY = math.random(-100, 100) / 100 + particle.Position.Y.Scale
		
		local tweenInfo = TweenInfo.new(
			math.random(15, 30) / 10,
			Enum.EasingStyle.Linear,
			Enum.EasingDirection.InOut,
			-1,
			true
		)
		
		local goal = {
			Position = UDim2.fromScale(
				math.clamp(destinationX, 0, 1),
				math.clamp(destinationY, 0, 1)
			)
		}
		
		local tween = TweenService:Create(particle, tweenInfo, goal)
		table.insert(tweens, tween)
		tween:Play()
	end
	
	return tweens
end

local function createFloatingParticles(container)
	for i = 1, 40 do
		local particle = Instance.new("Frame")
		particle.Name = "FloatingParticle"
		particle.BackgroundColor3 = Color3.fromRGB(30, 100, 210)
		particle.BorderSizePixel = 0
		
		local size = math.random(2, 8)
		particle.Size = UDim2.fromOffset(size, size)
		
		local xPos = math.random(0, 1000) / 1000
		local yPos = math.random(0, 1000) / 1000
		particle.Position = UDim2.fromScale(xPos, yPos)
		
		particle.BackgroundTransparency = math.random(7, 9) / 10
		particle.ZIndex = 22
		
		local corner = Instance.new("UICorner")
		corner.CornerRadius = UDim.new(1, 0)
		corner.Parent = particle
		
		particle.Parent = container
		
		local tweenInfo = TweenInfo.new(
			math.random(10, 20),
			Enum.EasingStyle.Linear,
			Enum.EasingDirection.InOut,
			-1,
			true
		)
		
		local goal = {
			Position = UDim2.fromScale(
				math.clamp(xPos + math.random(-30, 30) / 100, 0, 1),
				math.clamp(yPos + math.random(-30, 30) / 100, 0, 1)
			)
		}
		
		TweenService:Create(particle, tweenInfo, goal):Play()
	end
end

local function runLoadingSequence()
	local teleportEffect = createTeleportEffect()
	local launchFrame, launchButton = createLaunchScreen()
	local helperContainer, botImage, textLabel, progressBar, percentLabel, titleLabel, discordButton, discordIcon = createHelperBot()
	
	local particleTweens = animateParticles(teleportEffect.ParticleContainer)
	
	TweenService:Create(teleportEffect, TweenInfo.new(1), {BackgroundTransparency = 0}):Play()
	
	task.wait(1)
	
	TweenService:Create(teleportEffect.Overlay, TweenInfo.new(2), {BackgroundTransparency = 0}):Play()
	
	task.wait(3)
	
	for _, tween in ipairs(particleTweens) do
		tween:Cancel()
	end
	
	teleportEffect.Visible = false
	launchFrame.Visible = true
	
	TweenService:Create(launchFrame, TweenInfo.new(1), {BackgroundTransparency = 0}):Play()
	task.wait(0.5)
	TweenService:Create(launchFrame.Title, TweenInfo.new(1), {TextTransparency = 0}):Play()
	task.wait(0.5)
	TweenService:Create(launchButton, TweenInfo.new(1), {TextTransparency = 0}):Play()
	
	local buttonHover = false
	
	launchButton.MouseEnter:Connect(function()
		buttonHover = true
		TweenService:Create(launchButton, TweenInfo.new(0.3), {Size = UDim2.fromOffset(220, 55)}):Play()
	end)
	
	launchButton.MouseLeave:Connect(function()
		buttonHover = false
		TweenService:Create(launchButton, TweenInfo.new(0.3), {Size = UDim2.fromOffset(200, 50)}):Play()
	end)
	
	task.spawn(function()
		while task.wait(0.05) do
			if not launchButton.Parent then break end
			
			launchButton.Rotation = math.sin(tick() * 2) * 0.5
			
			if buttonHover then
				buttonGlow = launchButton:FindFirstChildOfClass("UIStroke")
				if buttonGlow then
					buttonGlow.Thickness = 2 + math.sin(tick() * 5) * 1
				end
			end
		end
	end)
	
	local botMessages = {
		"Hello there! I'm helper bot! I'll guide you how to use the script properly, so you won't have issues. As well, the script is doing final loading stages, to make sure It's working perfectly and you can use script safely! As well, it's required to bypass Anti-Cheat - So you won't get banned! Please, Do NOT leave the game while It's loading - Or it could result in potential data loss!",
		"This script has been optimized for performance and ease of use. Let me show you how to make the most of it.",
		"First, let's talk about the key features. You'll find everything neatly organized in the main menu.",
		"The script includes several powerful tools that will help you accomplish your tasks efficiently.",
		"Make sure to check out the settings menu where you can customize everything to your liking.",
		"The script automatically saves your preferences, so you don't have to reconfigure everything each time.",
		"I've implemented several optimization techniques to ensure smooth performance on all devices.",
		"The user interface has been designed to be intuitive and responsive, making navigation a breeze.",
		"Security features have been integrated to protect your data and ensure privacy.",
		"You can customize the appearance with various themes available in the settings menu.",
		"Advanced users can access additional features through the developer panel.",
		"Remember that you can always come back to the tutorial section if you need a refresher on how things work.",
		"I recommend starting with the basic functions before moving on to the more advanced features.",
		"Regular updates will be provided to enhance functionality and fix any issues.",
		"That's all for now! You're all set to begin. If you need any help, just look for my icon in the help section."
	}
	
	local botPositions = {
		UDim2.new(0.2, 0, 0.4, 0),
		UDim2.new(0.18, 0, 0.35, 0),
		UDim2.new(0.22, 0, 0.45, 0),
		UDim2.new(0.2, 0, 0.4, 0),
		UDim2.new(0.18, 0, 0.35, 0),
		UDim2.new(0.22, 0, 0.45, 0),
		UDim2.new(0.2, 0, 0.4, 0),
		UDim2.new(0.18, 0, 0.35, 0),
		UDim2.new(0.22, 0, 0.45, 0),
		UDim2.new(0.2, 0, 0.4, 0),
		UDim2.new(0.18, 0, 0.35, 0),
		UDim2.new(0.22, 0, 0.45, 0),
		UDim2.new(0.2, 0, 0.4, 0),
		UDim2.new(0.18, 0, 0.35, 0),
		UDim2.new(0.22, 0, 0.45, 0)
	}
	
	launchButton.MouseButton1Click:Connect(function()
		TweenService:Create(launchButton, TweenInfo.new(0.2), {Size = UDim2.fromOffset(190, 45)}):Play()
		task.wait(0.1)
		TweenService:Create(launchButton, TweenInfo.new(0.2), {Size = UDim2.fromOffset(200, 50)}):Play()
		
		task.wait(0.3)
		
		TweenService:Create(launchFrame.Title, TweenInfo.new(0.7), {TextTransparency = 1}):Play()
		TweenService:Create(launchButton, TweenInfo.new(0.7), {TextTransparency = 1}):Play()
		
		task.wait(1)
		launchFrame.Visible = false
		helperContainer.Visible = true
		
		createFloatingParticles(helperContainer)
		
		TweenService:Create(botImage, TweenInfo.new(1), {
			ImageTransparency = 0,
			Position = botPositions[1],
			Size = UDim2.fromOffset(120, 120)
		}):Play()
		
		TweenService:Create(helperContainer.TitleContainer.TitleLabel, TweenInfo.new(1), {
			TextTransparency = 0
		}):Play()

		TweenService:Create(discordButton, TweenInfo.new(1), {
			TextTransparency = 0
		}):Play()
		
		TweenService:Create(discordIcon, TweenInfo.new(1), {
			ImageTransparency = 0
		}):Play()
		
		discordButton.MouseEnter:Connect(function()
			TweenService:Create(discordButton, TweenInfo.new(0.3), {
				Size = UDim2.fromOffset(210, 45),
				BackgroundColor3 = Color3.fromRGB(107, 118, 255)
			}):Play()
		end)
		
		discordButton.MouseLeave:Connect(function()
			TweenService:Create(discordButton, TweenInfo.new(0.3), {
				Size = UDim2.fromOffset(200, 40),
				BackgroundColor3 = Color3.fromRGB(88, 101, 242)
			}):Play()
		end)
		
		discordButton.MouseButton1Click:Connect(function()
			local inviteLink = "https://discord.gg/6wFCr9P4Zr"
			
			pcall(function()
				setclipboard(inviteLink)
			end)
			
			discordButton.Text = "Copied to clipboard!"
			TweenService:Create(discordButton, TweenInfo.new(0.2), {
				Size = UDim2.fromOffset(190, 35)
			}):Play()
			
			task.wait(0.1)
			TweenService:Create(discordButton, TweenInfo.new(0.2), {
				Size = UDim2.fromOffset(200, 40)
			}):Play()
			
			task.wait(2)
			discordButton.Text = "Join Discord"
		end)
		
		task.wait(1)
		
		local totalDuration = 300
		local messageDuration = totalDuration / #botMessages
		local progress = 0
		
		local function updateChatBubblePosition(botPos)
			return UDim2.new(
				botPos.X.Scale + 0.15, 
				0, 
				botPos.Y.Scale, 
				0
			)
		end
		
		for i, message in ipairs(botMessages) do
			local newPosition = botPositions[i]
			
			local floatTween = TweenService:Create(botImage, TweenInfo.new(0.8, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
				Position = newPosition
			})
			floatTween:Play()
			
			local bubblePos = updateChatBubblePosition(newPosition)
			TweenService:Create(helperContainer.ChatBubble, TweenInfo.new(0.8, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
				Position = bubblePos
			}):Play()
			
			task.wait(0.8)
			
			typewriterEffect(textLabel, message, 0.02)
			
			task.spawn(function()
				local startTime = tick()
				local animDuration = messageDuration - 2
				
				while tick() - startTime < animDuration do
					local offset = math.sin(tick() * 2) * 3
					TweenService:Create(botImage, TweenInfo.new(0.1), {
						Position = UDim2.new(newPosition.X.Scale, offset, newPosition.Y.Scale, math.abs(offset/2))
					}):Play()
					task.wait(0.1)
				end
				
				TweenService:Create(botImage, TweenInfo.new(0.3), {
					Position = newPosition
				}):Play()
			end)
			
			local startProgress = progress
			local endProgress = i / #botMessages
			local progressUpdateInterval = 1
			local progressSteps = messageDuration / progressUpdateInterval
			
			for j = 1, progressSteps do
				progress = startProgress + (endProgress - startProgress) * (j / progressSteps)
				TweenService:Create(progressBar, TweenInfo.new(0.2), {
					Size = UDim2.fromScale(progress, 1)
				}):Play()
				percentLabel.Text = math.floor(progress * 100) .. "%"
				task.wait(progressUpdateInterval)
			end
			
			if i < #botMessages then
				TweenService:Create(textLabel, TweenInfo.new(0.5), {TextTransparency = 1}):Play()
				task.wait(0.5)
				textLabel.Text = ""
				TweenService:Create(textLabel, TweenInfo.new(0.5), {TextTransparency = 0}):Play()
				task.wait(0.5)
			end
		end
		
		task.wait(1)
		
		TweenService:Create(botImage, TweenInfo.new(1), {
			ImageTransparency = 1,
			Position = UDim2.new(botImage.Position.X.Scale, -120, botImage.Position.Y.Scale, 0)
		}):Play()
		
		TweenService:Create(helperContainer.ChatBubble, TweenInfo.new(1), {
			BackgroundTransparency = 1,
			Position = UDim2.new(helperContainer.ChatBubble.Position.X.Scale + 0.2, 0, helperContainer.ChatBubble.Position.Y.Scale, 0)
		}):Play()
		
		TweenService:Create(helperContainer.ChatBubble.BubbleArrow, TweenInfo.new(1), {
			BackgroundTransparency = 1
		}):Play()
		
		TweenService:Create(textLabel, TweenInfo.new(1), {TextTransparency = 1}):Play()
		
		TweenService:Create(helperContainer.LoadingBar, TweenInfo.new(1), {
			BackgroundTransparency = 1
		}):Play()
		
		TweenService:Create(progressBar, TweenInfo.new(1), {
			BackgroundTransparency = 1
		}):Play()
		
		TweenService:Create(percentLabel, TweenInfo.new(1), {
			TextTransparency = 1
		}):Play()

		TweenService:Create(discordButton, TweenInfo.new(1), {
			TextTransparency = 1
		}):Play()
		
		TweenService:Create(discordIcon, TweenInfo.new(1), {
			ImageTransparency = 1
		}):Play()
		
		task.wait(1.5)
		
		local finalFade = Instance.new("Frame")
		finalFade.Name = "FinalFade"
		finalFade.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
		finalFade.BackgroundTransparency = 1
		finalFade.Size = UDim2.fromScale(1, 1)
		finalFade.ZIndex = 100
		finalFade.Parent = loadingGui
		
		TweenService:Create(finalFade, TweenInfo.new(1), {BackgroundTransparency = 0}):Play()
		
		task.wait(1.5)
		
		loadingGui:Destroy()
	end)
end

local function initializeConnection()
	local renderSteppedConn = nil
	
	renderSteppedConn = RunService.RenderStepped:Connect(function()
		renderSteppedConn:Disconnect()
		runLoadingSequence()
	end)
end

initializeConnection()
