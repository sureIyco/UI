repeat task.wait() until game:IsLoaded()

local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")

local player = Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

local loadingGui = Instance.new("ScreenGui")
loadingGui.Name = "LoadingGui"
loadingGui.IgnoreGuiInset = true
loadingGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
loadingGui.Parent = game.CoreGui

local inputBlocker = Instance.new("Frame")
inputBlocker.Name = "InputBlocker"
inputBlocker.BackgroundTransparency = 1
inputBlocker.Size = UDim2.fromScale(1, 1)
inputBlocker.ZIndex = 1
inputBlocker.Parent = loadingGui

local function createTeleportEffect()
	local teleportFrame = Instance.new("Frame")
	teleportFrame.Name = "TeleportEffect"
	teleportFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	teleportFrame.BackgroundTransparency = 1
	teleportFrame.Size = UDim2.fromScale(1, 1)
	teleportFrame.ZIndex = 10
	teleportFrame.Parent = loadingGui
	
	local particleContainer = Instance.new("Frame")
	particleContainer.Name = "ParticleContainer"
	particleContainer.BackgroundTransparency = 1
	particleContainer.Size = UDim2.fromScale(1, 1)
	particleContainer.Parent = teleportFrame
	
	for i = 1, 150 do
		local particle = Instance.new("Frame")
		particle.Name = "Particle"
		particle.BackgroundColor3 = Color3.fromRGB(41, 128, 255)
		particle.BorderSizePixel = 0
		
		local size = math.random(2, 6)
		particle.Size = UDim2.fromOffset(size, size)
		
		local xPos = math.random(0, 1000) / 1000
		local yPos = math.random(0, 1000) / 1000
		particle.Position = UDim2.fromScale(xPos, yPos)
		
		particle.BackgroundTransparency = math.random(0, 7) / 10
		particle.ZIndex = 11
		particle.Parent = particleContainer
	end
	
	local overlay = Instance.new("Frame")
	overlay.Name = "Overlay"
	overlay.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	overlay.BackgroundTransparency = 1
	overlay.Size = UDim2.fromScale(1, 1)
	overlay.ZIndex = 12
	overlay.Parent = teleportFrame
	
	return teleportFrame
end

local function createLaunchScreen()
	local launchFrame = Instance.new("Frame")
	launchFrame.Name = "LaunchScreen"
	launchFrame.BackgroundColor3 = Color3.fromRGB(15, 15, 20)
	launchFrame.Size = UDim2.fromScale(1, 1)
	launchFrame.Visible = false
	launchFrame.ZIndex = 15
	launchFrame.Parent = loadingGui
	
	local uiGradient = Instance.new("UIGradient")
	uiGradient.Color = ColorSequence.new({
		ColorSequenceKeypoint.new(0, Color3.fromRGB(30, 30, 40)),
		ColorSequenceKeypoint.new(1, Color3.fromRGB(15, 15, 20))
	})
	uiGradient.Rotation = 45
	uiGradient.Parent = launchFrame
	
	local titleLabel = Instance.new("TextLabel")
	titleLabel.Name = "Title"
	titleLabel.BackgroundTransparency = 1
	titleLabel.Position = UDim2.fromScale(0.5, 0.4)
	titleLabel.AnchorPoint = Vector2.new(0.5, 0.5)
	titleLabel.Size = UDim2.fromScale(0.8, 0.2)
	titleLabel.Font = Enum.Font.GothamBold
	titleLabel.Text = "Blox Fruits Script"
	titleLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
	titleLabel.TextScaled = true
	titleLabel.TextTransparency = 1
	titleLabel.ZIndex = 16
	titleLabel.Parent = launchFrame
	
	local launchButton = Instance.new("TextButton")
	launchButton.Name = "LaunchButton"
	launchButton.AnchorPoint = Vector2.new(0.5, 0.5)
	launchButton.BackgroundColor3 = Color3.fromRGB(41, 128, 255)
	launchButton.Position = UDim2.fromScale(0.5, 0.6)
	launchButton.Size = UDim2.fromOffset(200, 50)
	launchButton.Font = Enum.Font.GothamSemibold
	launchButton.Text = "Launch Script."
	launchButton.TextColor3 = Color3.fromRGB(0, 255, 255)
	launchButton.TextScaled = true
	launchButton.TextTransparency = 1
	launchButton.AutoButtonColor = false
	launchButton.ZIndex = 16
	launchButton.Parent = launchFrame
	
	local buttonCorner = Instance.new("UICorner")
	buttonCorner.CornerRadius = UDim.new(0, 10)
	buttonCorner.Parent = launchButton
	
	local buttonGlow = Instance.new("UIStroke")
	buttonGlow.Color = Color3.fromRGB(65, 143, 255)
	buttonGlow.Thickness = 2
	buttonGlow.Parent = launchButton
	
	local buttonGradient = Instance.new("UIGradient")
	buttonGradient.Color = ColorSequence.new({
		ColorSequenceKeypoint.new(0, Color3.fromRGB(65, 143, 255)),
		ColorSequenceKeypoint.new(1, Color3.fromRGB(41, 128, 255))
	})
	buttonGradient.Rotation = 90
	buttonGradient.Parent = launchButton
	
	return launchFrame, launchButton
end

